@using static dn32.infra.DnDocExtension

@{
    ViewData["Title"] = "Autenticação".G();
}

<div class="auth-container">

    <form method="post" action="/dndoc/Sair">
        <div class="form-group">
            <label for="user">@("Token".G())</label>
            <textarea class="form-control" name="token" style="height: 361px;">@ViewBag.Token</textarea>
        </div>
       
        <button type="submit" class="btn btn-primary">@("Sair".G())</button>
    </form>

</div>
